        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.PrincipalActivity" android:label="@string/app_name"/>
        <activity android:name=".Activity.PassagemParametroActivity.PassagemParametroTela1" android:label="@string/app_name"/>
        <activity android:name=".Activity.PassagemParametroActivity.PassagemParametroTela2" android:label="@string/app_name"/>
        <activity android:name=".Activity.FecharActivity.FecharActivityTela1" android:label="@string/app_name"/>
        <activity android:name=".Activity.ListaCustomizada.ExemploSmileAdapter" android:label="@string/app_name"/>
        <activity android:name=".Intent.IntentPrincipal" android:label="@string/app_name"/>
        <activity android:name=".Intent.AbrirBrowser.AbrirBrowser" android:label="@string/app_name"/>
        <activity android:name=".Intent.FazerLigacao.FazerLigacao" android:label="@string/app_name"/>

    </application>

</manifest>
